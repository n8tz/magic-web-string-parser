#!/usr/bin/env node
'use strict';
const fs            = require('fs'),
      data          = fs.readFileSync(0, 'utf-8'),
      decEverything = require('../');
let output;
if ( data.length )
	output = decEverything.stringify(JSON.parse(data.substring(0, data.length - 1)));
else
	output = decEverything.stringify(JSON.parse(process.argv[3]));

console.log(output);
